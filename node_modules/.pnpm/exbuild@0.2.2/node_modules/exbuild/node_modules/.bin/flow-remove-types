#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/admin/Desktop/git-project/learning-vue-source-code/node_modules/.pnpm/flow-remove-types@2.295.0/node_modules/flow-remove-types/node_modules:/mnt/c/Users/admin/Desktop/git-project/learning-vue-source-code/node_modules/.pnpm/flow-remove-types@2.295.0/node_modules:/mnt/c/Users/admin/Desktop/git-project/learning-vue-source-code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/admin/Desktop/git-project/learning-vue-source-code/node_modules/.pnpm/flow-remove-types@2.295.0/node_modules/flow-remove-types/node_modules:/mnt/c/Users/admin/Desktop/git-project/learning-vue-source-code/node_modules/.pnpm/flow-remove-types@2.295.0/node_modules:/mnt/c/Users/admin/Desktop/git-project/learning-vue-source-code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../flow-remove-types@2.295.0/node_modules/flow-remove-types/flow-remove-types" "$@"
else
  exec node  "$basedir/../../../../../flow-remove-types@2.295.0/node_modules/flow-remove-types/flow-remove-types" "$@"
fi
